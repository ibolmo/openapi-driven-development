{
  "compilerOptions": {
    "baseUrl": "./src",
    "declaration": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "lib": ["ESNext"],
    "module": "ESNext",
    "moduleResolution": "node",
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "outDir": "dist",
    "removeComments": false,
    "sourceMap": true,
    "strict": true,
    "resolveJsonModule": true,
    "target": "ES2020"
  },
  "exclude": []
}
